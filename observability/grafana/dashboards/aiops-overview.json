{
  "uid": "aiops-overview",
  "title": "AIOps â€” Overview",
  "tags": [
    "aiops",
    "phase7"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "label": "Service",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "query": {
          "query": "label_values(app_requests_total, service)",
          "refId": "A"
        },
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "RPS (requests/sec) by service",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by(service) (rate(app_requests_total{service=~\"$service\",endpoint=\"/simulate\"}[1m]))",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "EPS (errors/sec) by service",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by(service) (rate(app_errors_total{service=~\"$service\",endpoint=\"/simulate\"}[1m]))",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Error ratio (EPS/RPS)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum by(service) (rate(app_errors_total{service=~\"$service\",endpoint=\"/simulate\"}[1m]))) / clamp_min((sum by(service) (rate(app_requests_total{service=~\"$service\",endpoint=\"/simulate\"}[1m]))), 0.000001)",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "p95 latency (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by(le,service) (rate(app_request_latency_seconds_bucket{service=~\"$service\",endpoint=\"/simulate\"}[1m])))",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Logs ERROR count (Loki, 1m window)",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by(service) (count_over_time({container=~\".*simulator-($service).*\"} | json | level=\"ERROR\" [1m]))",
          "legendFormat": "{{service}}"
        }
      ]
    }
  ]
}