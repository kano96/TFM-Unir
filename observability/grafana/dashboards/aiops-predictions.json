{
  "uid": "aiops-predictions",
  "title": "AIOps \u2014 Prediction Timeline (Predictor)",
  "tags": [
    "aiops",
    "predictor",
    "phase7"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "custom",
        "label": "Service",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "query": "auth,user,orders",
        "allValue": ".*"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Incident Probability",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "aiops_incident_probability{service=~\"$service\"}",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Incident Flag (prob >= \u03c4*)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 6
      },
      "targets": [
        {
          "refId": "A",
          "expr": "aiops_incident_flag{service=~\"$service\"}",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "id": 3,
      "type": "logs",
      "title": "Predictor Service Logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "x": 0,
        "y": 14,
        "w": 24,
        "h": 10
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{container=~\".*predictor.*\"}"
        }
      ]
    }
  ]
}